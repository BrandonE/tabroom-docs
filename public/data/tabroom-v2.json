{
    "name": "Tabroom v2 API",
    "description": "An API for Tabroom.com, version 2",
    "protocol": "rest",
    "basePath": "https://tabroom-node.herokuapp.com",
    "publicPath": "",
    "auth": {
        "key": {
            "param": "key"
        }
    },
    "headers": {
        "Content-Type": "application/vnd.tabroom+json; version=2",
        "Accept": "application/vnd.tabroom+json; version=2"
    },
    "resources": {
        "Tournaments": {
            "methods": {
                "All": {
                    "name": "All",
                    "path": "/tournaments/",
                    "httpMethod": "GET",
                    "description": "Gets all of the tournaments.",
                    "parameters": {
                        "fields": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "The attribute(s) to select, delimited by comma. Leaving this blank selects everything."
                        },
                        "sort": {
                            "type": "string",
                            "required": false,
                            "default": "id",
                            "description": "The attribute(s) to sort the tournaments by, delimited by comma. Preceding an attribute with a minus sign results in a descending order."
                        },
                        "id": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by id"
                        },
                        "name": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by name"
                        },
                        "start": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by start"
                        },
                        "end": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by end"
                        },
                        "reg_start": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by reg_start"
                        },
                        "reg_end": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by reg_end"
                        },
                        "timestamp": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by timestamp"
                        },
                        "hidden": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by hidden"
                        },
                        "approved": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by approved"
                        },
                        "webname": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by webname"
                        },
                        "tz": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by tz"
                        },
                        "state": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by state"
                        },
                        "country": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by country"
                        },
                        "foreign_site": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by foreign_site"
                        },
                        "foreign_id": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by foreign_id"
                        },
                        "created_by": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "Filter by created_by"
                        }
                    }
                },
                "Individual": {
                    "name": "Individual",
                    "path": "/tournaments/{ids}",
                    "httpMethod": "GET",
                    "description": "Gets a tournament or set of tournaments.",
                    "parameters": {
						"ids": {
							"type": "string",
                            "required": true,
                            "default": "",
                            "description": "The id(s) of the tournament(s) to get, delimited by comma."
						},
                        "fields": {
                            "type": "string",
                            "required": false,
                            "default": "",
                            "description": "The attribute(s) to select, delimited by comma. Leaving this blank selects everything."
                        }
                    }
                }
            }
        }
    }
}